{
  "meta": {
    "$_GET": [
      {
        "type": "text",
        "name": "offset"
      },
      {
        "type": "text",
        "name": "limit"
      },
      {
        "type": "text",
        "name": "sort"
      },
      {
        "type": "text",
        "name": "dir"
      },
      {
        "type": "text",
        "name": "scheme_id"
      }
    ]
  },
  "exec": {
    "steps": [
      {
        "name": "",
        "module": "auth",
        "action": "restrict",
        "options": {
          "provider": "securityLogin",
          "permissions": [
            "write"
          ],
          "loginUrl": "login",
          "forbiddenUrl": "403-forbidden"
        }
      },
      {
        "name": "AllCommisionScheme",
        "module": "dbupdater",
        "action": "custom",
        "options": {
          "connection": "postgres_gcpServer",
          "sql": {
            "query": "select tssc.id, ms.id as service_id, ms.service_name, tssc.status \nfrom m_service ms \nleft join t_service_scheme_control tssc on tssc.service_id = ms.id and tssc.scheme_id = @scheme \norder by ms.service_name asc",
            "params": [
              {
                "name": "@scheme",
                "value": "{{$_GET.scheme_id}}",
                "test": "1"
              }
            ]
          }
        },
        "output": true,
        "meta": [
          {
            "name": "id",
            "type": "number"
          },
          {
            "name": "service_name",
            "type": "text"
          },
          {
            "name": "status",
            "type": "text"
          }
        ],
        "outputType": "array"
      }
    ]
  }
}