{
  "exec": {
    "steps": [
      "connections/postgres_gcpServer",
      {
        "name": "_query",
        "module": "dbupdater",
        "action": "custom",
        "output": true,
        "options": {
          "connection": "postgres_gcpServer",
          "sql": {
            "query": "SELECT mscu.id, mscu.user_mobile, mscu.user_name, mscu.ip, mscu.user_mobile, mscu.message, mscu.status, mscu.created_on::text\nFROM m_swpay_contact_us mscu \nWHERE mscu.created_on BETWEEN @fromdate AND @todate ORDER BY mscu.id desc;",
            "params": [
              {
                "name": "@fromdate",
                "value": "2022-05-26 00:00:00"
              },
              {
                "name": "@todate",
                "value": "2022-06-26 23:59:59"
              }
            ]
          }
        }
      }
    ],
    "catch": {
      "name": "_error",
      "module": "core",
      "action": "setvalue",
      "output": true,
      "options": {
        "value": "{{$_ERROR}}"
      }
    }
  }
}