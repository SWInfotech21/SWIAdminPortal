      {
        "name": "AllCommisionScheme",
        "module": "dbupdater",
        "action": "custom",
        "options": {
          "connection": "postgres_gcpServer",
          "sql": {
            "query": "select mo.id as op_id, mo.operator_name, ms.service_name, mc.id as commision_id, mc.scheme_id, mc.comm_per, mc.comm_val, mc.service_charge_per, mc.service_charge_val, mc.is_active \nfrom m_operator mo \ninner join m_service ms on ms.id = mo.service_id \nleft join m_commission mc on mc.operator_id = mo.id and mc.scheme_id = @scheme \norder by mo.operator_name asc ",
            "params": [
              {
                "name": "@scheme",
                "value": "{{$_GET.scheme_id}}"
              }
            ]
          }
        },
        "output": true,
        "meta": [],
        "outputType": "array"
      }
