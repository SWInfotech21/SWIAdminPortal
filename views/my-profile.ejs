<!-- Wappler include head-page="layouts/main" is="dmx-app" id="myprofile" appConnect="local" -->
<meta name="ac:route" content="/my-profile">
<div class="container">
    <div class="row">
        <div class="col-12">
            <h1 class="mt-4">Profile</h1>

            <ol class="breadcrumb mb-4">
                <li class="breadcrumb-item active">User Info./ {{scAdminBalance.data.query.user_id}}</li>
            </ol>
        </div>
    </div>
    <div class="row">
        <div class="col-12 col-sm-4 mb-3">
            <label class="fw-bold" for="inlineFormInputGroup">Username</label>
            <div class="input-group mb-2">
                <div class="input-group-prepend">
                    <div class="input-group-text bg-body">
                        <i class="far fa-user"></i>
                    </div>
                </div>
                <input type="text" class="form-control form-control-plaintext" id="inlineFormInputGroupName" disabled="true" readonly="true" dmx-bind:value="scAdminBalance.data.query.user_fname">
            </div>
        </div>
        <div class="col-12 col-sm-4 mb-3">
            <label class="fw-bold" for="inlineFormInputGroup">Mobile</label>
            <div class="input-group mb-2">
                <div class="input-group-prepend">
                    <div class="input-group-text bg-body">
                        <i class="fas fa-mobile-alt"></i>
                    </div>
                </div>
                <input type="text" class="form-control form-control-plaintext" id="inlineFormInputGroupMobile" disabled="true" readonly="true" dmx-bind:value="scAdminBalance.data.query.user_mobile">
            </div>
        </div>
        <div class="col-12 col-sm-4 mb-3">
            <label class="fw-bold" for="inlineFormInputGroup">Email</label>
            <div class="input-group mb-2">
                <div class="input-group-prepend">
                    <div class="input-group-text bg-body">
                        <i class="far fa-envelope"></i>
                    </div>
                </div>
                <input type="text" class="form-control form-control-plaintext" id="inlineFormInputGroupEmail" disabled="true" readonly="true" dmx-bind:value="scAdminBalance.data.query.user_email">
            </div>
        </div>
        <div class="col-12 col-sm-4 mb-3">
            <label class="fw-bold" for="inlineFormInputGroup">PAN No.</label>
            <div class="input-group mb-2">
                <div class="input-group-prepend">
                    <div class="input-group-text bg-body">
                        <i class="far fa-address-card"></i>
                    </div>
                </div>
                <input type="text" class="form-control form-control-plaintext" id="inlineFormInputGroupPAN" disabled="true" readonly="true" dmx-bind:value="scAdminBalance.data.query.pan_no">
            </div>
        </div>
        <div class="col-12 col-sm-4 mb-3">
            <label class="fw-bold" for="inlineFormInputGroup">Adhar No.</label>
            <div class="input-group mb-2">
                <div class="input-group-prepend">
                    <div class="input-group-text bg-body">
                        <i class="fas fa-id-card"></i>
                    </div>
                </div>
                <input type="text" class="form-control form-control-plaintext" id="inlineFormInputGroupAadhar" disabled="true" readonly="true" dmx-bind:value="scAdminBalance.data.query.aadhar_no">
            </div>
        </div>
        <div class="col-12 col-sm-4 mb-3">
            <label class="fw-bold" for="inlineFormInputGroup">PIN</label>
            <div class="input-group mb-2">
                <div class="input-group-prepend">
                    <div class="input-group-text bg-body">
                        <i class="fas fa-home"></i>
                    </div>
                </div>
                <input type="text" class="form-control form-control-plaintext" id="inlineFormInputGroupPin" disabled="true" readonly="true" dmx-bind:value="scAdminBalance.data.query.pin">
            </div>
        </div>
        <div class="col-12 col-sm-4 mb-3">
            <label class="fw-bold" for="inlineFormInputGroup">State</label>
            <div class="input-group mb-2">
                <div class="input-group-prepend">
                    <div class="input-group-text bg-body">
                        <i class="fas fa-address-card"></i>
                    </div>
                </div>
                <input type="text" class="form-control form-control-plaintext" id="inlineFormInputGroupState" disabled="true" readonly="true" dmx-bind:value="scAdminBalance.data.query.state">
            </div>
        </div>
        <div class="col-12 col-sm-4 mb-3">
            <label class="fw-bold" for="inlineFormInputGroup">District</label>
            <div class="input-group mb-2">
                <div class="input-group-prepend">
                    <div class="input-group-text bg-body">
                        <i class="fas fa-house-damage"></i>
                    </div>
                </div>
                <input type="text" class="form-control form-control-plaintext" id="inlineFormInputGroupDistrict" disabled="true" readonly="true" dmx-bind:value="scAdminBalance.data.query.district">
            </div>
        </div>
        <div class="col-12 col-sm-4 mb-3">
            <label class="fw-bold" for="inlineFormInputGroup">Address</label>
            <div class="input-group mb-2">
                <div class="input-group-prepend">
                    <div class="input-group-text bg-body">
                        <i class="far fa-address-card"></i>
                    </div>
                </div>
                <textarea type="text" class="form-control form-control-plaintext" id="inlineFormInputGroupAddres" disabled="true" readonly="true" dmx-bind:value="scAdminBalance.data.query.address"></textarea>
            </div>
        </div>
    </div>
</div>